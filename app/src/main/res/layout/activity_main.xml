<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="#000000"
    android:visibility="visible"
    tools:context=".Main.MainActivity"
    tools:visibility="visible">









    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="310dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/change2"
        android:nextFocusLeft="@drawable/change"
        android:screenReaderFocusable="true"
        android:src="@drawable/change1"

        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="331dp"
        android:layout_marginTop="23dp"
        android:src="@drawable/change"

        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="285dp"
        android:layout_marginTop="13dp"
        android:src="@drawable/wifi" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="260dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/signal"


        />


    <TextView
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:text="9:41"
        android:textColor="#FFFBFB"
        android:textStyle="bold"

        />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="350dp"
        android:layout_height="250dp"

        android:layout_marginLeft="5dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/background">
        <TextView
            android:id="@+id/textViewCategory"
            style="@style/category1"


            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginRight="140dp"
            android:text="Category"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />




        <TextView
            android:id="@+id/explore_the"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="70dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="80dp"
            android:text="Explore the nightlife of your city"

            android:lineSpacingExtra="4sp"
            android:gravity="center_horizontal|center_vertical"
            android:textStyle="bold"
            android:textAllCaps="false"
            android:textSize="15dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent" />

        <ImageView
            android:id="@+id/vector_stro"
            android:layout_width="40dp"
            android:layout_height="40dp"

            android:layout_marginLeft="305dp"
            android:layout_marginTop="11dp"
            android:background="@drawable/heart"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"


            />

        <ImageView

            android:id="@+id/vectorHeart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:visibility="gone"
            android:background="@drawable/ic_baseline_favorite_24"
            android:layout_marginLeft="305dp"
            android:layout_marginTop="11dp"


            />









        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="190dp"
            android:text="Free"
            android:textAllCaps="false"
            android:textSize="22dp"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"


            />

        <TextView

            android:id="@+id/textPrice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="33dp"
            android:layout_marginTop="215dp"
            android:text="Price &#128176;"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"


            />



        <ImageView
            android:id="@+id/percon2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="135dp"
            android:layout_marginTop="188dp"
            android:background="@drawable/circle"
            android:scrollX="-5dp"
            android:src="@drawable/ic_group_black_24dp"
             android:visibility="visible"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/percon1"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="188dp"

            android:background="@drawable/circle"
            android:scrollX="-5dp"
            android:scrollY="-5dp"
            android:src="@drawable/person"

            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/percon3"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="170dp"
            android:layout_marginTop="188dp"

            android:background="@drawable/circle"
            android:src="@drawable/groupv"

            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/percon4"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="203dp"
            android:layout_marginTop="188dp"

            android:background="@drawable/circle"
            android:src="@drawable/groupvv"

            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="106dp"
            android:layout_marginTop="220dp"
            android:text="Participants"

            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ProgressBar

      android:id="@+id/progress_bar"
            android:layout_width="90dp"
            android:layout_height="10dp"
           style="@style/Widget.AppCompat.ProgressBar.Horizontal"

              android:background="@drawable/pb_drawable_size"
            android:layout_marginLeft="250dp"
            android:layout_marginTop="205dp"
             android:max="100"
            app:layout_constraintLeft_toLeftOf="parent"

            app:layout_constraintTop_toTopOf="parent"


            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="260dp"
            android:layout_marginTop="220dp"
            android:text="Accessibility"

            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button

        android:onClick="TonextTo"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="335dp"

        android:background="@drawable/krug2" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="405dp"
        android:text="Category"
        android:textColor="#FFFFFF"
        android:textStyle="italic"

        />



    <Spinner

        android:id="@+id/Change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="430dp"
        android:background="@drawable/shadow"


        >


    </Spinner>


    <TextView
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginLeft="194dp"
        android:layout_marginTop="410dp"
        android:text="Price"
        android:textColor="#FFFFFF"


        />


    <TextView
        android:id="@+id/PriceTextAcc"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="400dp"
        android:background="@drawable/background"
        android:layout_marginLeft="250dp"


        android:text="0-9"
        android:textSize="20sp"
        android:gravity="center"
        />




    <me.bendik.simplerangeview.SimpleRangeView

        android:id="@+id/PriceRangeBar"
        android:layout_width="200dp"
        android:layout_height="wrap_content"

        android:layout_marginLeft="180dp"
        android:layout_marginTop="430dp"

        app:count="10"
        app:end="7"
        app:movable="true"
        app:start="3" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginLeft="194dp"
        android:layout_marginTop="510dp"
        android:text="Accessibility"
        android:textColor="#FFFFFF"


        />


  <TextView
      android:id="@+id/accibilText"

      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginTop="470dp"
      android:background="@drawable/background"
      android:layout_marginLeft="250dp"
      android:text="0.0-9.0"
android:textSize="20sp"
      android:gravity="center"
      >


  </TextView>


    <me.bendik.simplerangeview.SimpleRangeView

        android:id="@+id/accibilityRangeBar"

        android:layout_width="200dp"
        android:layout_height="wrap_content"

        android:layout_marginLeft="180dp"
        android:layout_marginTop="530dp"

        app:count="10"
        app:end="8"
        app:movable="true"
        app:start="4" />


    <Button
        android:layout_width="50dp"
        android:layout_height="40sp"
        android:layout_marginTop="560dp"
        android:layout_marginLeft="270dp"
        android:id="@+id/update"
        android:background="#FFC107"
        android:text="update"
        android:textAllCaps="false"


        />


</RelativeLayout>